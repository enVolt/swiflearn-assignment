# This will return Stack Trace with API Errors
NODE_ENV=develop

# Port, Application should listen on
PORT=3000

# Database Config 
DB_CONNECTION_STRING=postgresql://localhost:5432/swiflearn

# Can be generated from IAM Service of AWS Console
AWS_ACCESS_KEY=
AWS_SECRET_ACCESS_KEY=

# All queues should be in same region, for any queue pick the prefix as example format
QUEUE_URL_PREFIX=https://sqs.ap-southeast-1.amazonaws.com/204685349125/

# All sns topics should be in same region, for any sns topic prefix as example format
SNS_TOPIC_ARN_PREFIX=

EMAIL_PROCESS_QUEUE=develop_email_process_queue
EMAIL_LOW_PRIORITY_PROCESS_QUEUE=develop_email_low_priority_process_queue
EMAIL_TRACKER_STATUS=
DIGEST_EMAIL_PROCESS_QUEUE=

# Redis related config
REDIS_HOST=
REDIS_PORT=
REDIS_PASSWORD=
REDIS_DB=

# Error Tracking
BUGSNAG_KEY=

# Base Urls
CLIENT_BASE_URL=https://www.masmic.com
API_BASE_URL=http://api.masmic

